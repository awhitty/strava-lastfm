{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "module": "commonjs",
    "outDir": "./.tsbuild",
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ]
  },
  "files": [
    "src/routes/index.ts"
  ]
}
